<Window x:Class="HHBotWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HHBotWPF"
        mc:Ignorable="d"
        Title="HH API" Height="450" Width="800">
    <Grid>
        <Button x:Name="searchButton" Content="Поиск" HorizontalAlignment="Left" Margin="338,9,0,0" VerticalAlignment="Top" Height="22" Width="77" Click="searchButton_Click">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="321" Text="Программист" Height="19">
            <TextBox.Effect>
                <DropShadowEffect/>
            </TextBox.Effect>
        </TextBox>
        <ListBox x:Name="ResListBox" Margin="10,53,10,25"/>

    </Grid>
</Window>
